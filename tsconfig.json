{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "Bundler",
        "rootDir": "./src",
        "outDir": "./build",
        "removeComments": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true
    }
}
